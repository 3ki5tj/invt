      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "km"}], "+", "1"}], ")"}], "/", "n"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"Sin", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"2", "km"}], "+", "1"}], ")"}], "l", " ", 
         RowBox[{"Pi", "/", "n"}]}], "]"}], "/", 
       RowBox[{"Sin", "[", 
        RowBox[{"l", " ", 
